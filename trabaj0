package colorBox;

public class Persona{
    public String nombre;
    public long id;
    Persona(){}
    Persona(String name,long iden){
        nombre=name;
        id=iden;
    }
    Persona(long ide,String name){
        nombre=name;
        id=iden;
    }
}

public class Empleado extends Persona{
	public int sueldo,empleadoNum,horas;
	String cargo;
	Empleado(String nombre,String cargo,int id){
		this.nombre=nombre;
		this.id=id;
		this.cargo=cargo;
	}
	public void denominarSueldo(int horas) {
		CEO.asignarSueldo(horas);
	}
}
